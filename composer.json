{
    "name": "dft/silvercommerce-google-reviews-optin",
    "description": "Adds a Google reviews opt in survey to the complete page of a SilverCommerce store",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/Dean-Forest-Tech/silvercommerce-google-reviews-optin",
    "keywords": [
        "silverstripe",
        "TagManager",
        "Google Reviews"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dean Forest Tech",
            "homepage": "https://dean-forest-tech.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/Dean-Forest-Tech/silvercommerce-google-reviews-optin/issues"
    },
    "require": {
        "sminnee/tagmanager": "1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "DFT\\SilverCommerce\\GoogleReviewsOptIn\\": "src/"
        }
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev"
}
